@{
    var list = Model as IEnumerable<NETCore_Lession04.Models.DataModels.Member>;
}
@model IEnumerable<NETCore_Lession04.Models.DataModels.Member>
<h1>Danh sách thành viên</h1>
<table class="tbale table-bordered">
    <thead>
        <tr>
            <th>MemberId</th>
            <th>UserName</th>
            <th>FullName</th>
            <th>Password</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MemberId</td>
                <td>@item.UserName</td>
                <td>@item.FullName</td>
                <td>@item.Password</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>

</table>
<table class="tbale table-bordered">
    <thead>
        <tr>
            <th>MemberId</th>
            <th>UserName</th>
            <th>FullName</th>
            <th>Password</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in list)
        {
            <tr>
                <td>@item.MemberId</td>
                <td>@item.UserName</td>
                <td>@item.FullName</td>
                <td>@item.Password</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>

</table>