@{
    ViewData["Title"] = "Products";
    var list = ViewData["list"] as List<Product>;
}
<h1> Products List</h1>
<h2>ViewData: @ViewData["list"]</h2>
<table class="table table-bordered">
    <thead class="bg-danger">
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>     
    </thead>
    <tbody>
        @foreach (var item in list)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>