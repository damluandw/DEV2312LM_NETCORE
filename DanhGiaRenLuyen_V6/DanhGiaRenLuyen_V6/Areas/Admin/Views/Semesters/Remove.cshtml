@model DanhGiaRenLuyen_V6.Models.DBModel.Semester

@{
    ViewData["Title"] = "Remove";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Xoá</h1>

<h3>Bạn có chắc chắn muốn xoá vĩnh viễn kì đánh giấ</h3>
<div>
    <h4>Kì đánh giá</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.SchoolYear)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SchoolYear)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateOpenStudent)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateOpenStudent)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateEndStudent)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateEndStudent)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateEndClass)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateEndClass)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateEndLecturer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateEndLecturer)
        </dd>
    </dl>

    <form asp-action="Remove">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index" asp-route-isActive="0">Quay lại</a>
    </form>
</div>

